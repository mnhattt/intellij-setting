<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="FONT_FAMILY" value="Monaco" />
    <option name="LINE_SPACING" value="1.05" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="HTML" value="false" />
        <entry key="JAVA" value="true" />
        <entry key="JSON" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="800" />
    <option name="SHOW_INTENTION_BULB" value="false" />
    <option name="IS_CARET_BLINKING" value="false" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_BLOCK_CURSOR" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="SHOW_NOTIFICATION_AFTER_REFORMAT_CODE_ACTION" value="false" />
    <option name="SHOW_NOTIFICATION_AFTER_OPTIMIZE_IMPORTS_ACTION" value="false" />
  </component>
  <component name="JsonEditorOptions">
    <option name="COMMA_ON_ENTER" value="false" />
    <option name="COMMA_ON_MATCHING_BRACES" value="false" />
    <option name="COMMA_ON_PASTE" value="false" />
    <option name="AUTO_QUOTE_PROP_NAME" value="false" />
    <option name="AUTO_WHITESPACE_AFTER_COLON" value="false" />
    <option name="ESCAPE_PASTED_TEXT" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="autoCloseTag" value="false" />
    <option name="automaticallyInsertClosingTag" value="false" />
    <option name="automaticallyInsertRequiredAttributes" value="false" />
    <option name="automaticallyInsertRequiredSubTags" value="false" />
    <option name="automaticallyStartAttribute" value="false" />
    <option name="insertQuotesForAttributeValue" value="false" />
    <option name="selectWholeCssIdentifierOnDoubleClick" value="false" />
    <option name="syncTagEditing" value="false" />
    <option name="tagTreeHighlightingEnabled" value="false" />
  </component>
</application>